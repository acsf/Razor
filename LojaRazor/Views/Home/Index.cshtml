<div class="hero-unit">
    <h2>Queima de estoque</h2>
    <p>
        Promoção na sessão monitores!
    </p>
</div>
<div class="row-fluid">
    @foreach (var prod in ViewBag.Produtos)
    {
        <div class="span4">
            <h2>@prod.Nome</h2>
            <p>@prod.Descricao</p>
            <p>@prod.Preco</p>
        </div>
    }
</div>

